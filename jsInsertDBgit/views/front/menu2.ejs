							}
							let selectedGroupId = selectElement.value;
							let source = $('#source').html();
							$('td[data-id="' + selectedGroupId + '"]').html(source);
							$('td[data-id="' + selectedGroupId + '"] select.pairNumber1').val(currentState[selectedGroupId].pairNumber1);
